{
  "scripts": {
    "1.初始化/依赖包": "npm install",
    "2.安装调试的apk": "robot-tools init",
    "3.加密/热补丁": "robot-tools deploy apk=false",
    "4.加密/热补丁/打APK包": "robot-tools deploy",
	"5.加密/热补丁/打APK包[本地]": "robot-tools deploy apk=local makerpath=~/AndroidStudioProjects/HBuilder-Throne/make_apk"
  },
  "dependencies": {
    "compressing": "^1.5.1",
    "request": "^2.88.2",
    "robot-tools": "^1.1.23",
    "webpack": "^5.4.0"
  }
}
